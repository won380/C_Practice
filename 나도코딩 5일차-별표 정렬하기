int main(void) {
	for (int i = 0; i < 5; i++) {
		for (int j = i; j < 5-1; j++) {
			printf(" ");
		}//공백출력
		for (int k = 0; k <= i; k++) {
			printf("*");
		}//별표출력
		printf("\n");

	}
	return 0;
}
